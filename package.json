{
  "name": "react-redux-boilerplate",
  "version": "1.10.0",
  "private": true,
  "engines": {
    "node": "~7",
    "npm": "~4"
  },
  "dependencies": {
    "axios": "0.17.1",
    "big.js": "5.0.3",
    "classnames": "2.2.5",
    "compression": "1.7.1",
    "cross-env": "5.1.1",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "express": "4.16.2",
    "getenv": "0.7.0",
    "helmet": "3.9.0",
    "heroku-ssl-redirect": "0.0.4",
    "history": "4.7.2",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "4.1.2",
    "prop-types": "15.6.0",
    "react": "16.1.1",
    "react-dom": "16.1.1",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.8",
    "react-scripts": "1.0.17",
    "recompose": "0.26.0",
    "redux": "3.7.2",
    "redux-i18n": "1.5.10",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start-dev": "npm-run-all -p watch-css start-js",
    "start": "node server.js",
    "heroku-postbuild": "npm run build",
    "clear-build": "rm -rf build-dev && rm -rf build-prod",
    "build-dev": "REACT_APP_BUILD_ENV=development && react-scripts build && mv build build-dev",
    "build-prod": "REACT_APP_BUILD_ENV=production && react-scripts build && mv build build-prod",
    "build": "npm run build-css && npm run clear-build && npm run build-dev && npm run build-prod",
    "build-css": "node-sass-chokidar src/scss/index.scss -o src/css/",
    "watch-css": "node-sass-chokidar src/scss/ -o src/css/ --watch --recursive",
    "lint": "eslint ./src/**/*.js",
    "test": "cross-env NODE_ENV=test CI=true react-scripts test --env=jsdom",
    "cover": "npm test -- --coverage"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "eslint": "4.11.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.5.1",
    "pre-commit": "1.2.2",
    "react-test-renderer": "16.1.1",
    "redux-devtools": "3.4.1",
    "sinon": "4.1.2"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/components/Router/Router.js"
    ]
  },
  "keywords": [
    "react",
    "redux",
    "redux-thunk",
    "react-redux-boilerplate",
    "react-docker",
    "react-travis",
    "react-heroku"
  ]
}
