{
  "name": "react-redux-boilerplate",
  "version": "1.2.2",
  "private": true,
  "engines": {
    "node": "~7",
    "npm": "~4"
  },
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "3.1.0",
    "babel-runtime": "6.26.0",
    "big.js": "5.0.3",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.3.0",
    "compression": "1.7.1",
    "coveralls": "3.0.0",
    "cross-env": "5.1.1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "express": "4.16.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "4.0.2",
    "getenv": "0.7.0",
    "helmet": "3.9.0",
    "heroku-ssl-redirect": "0.0.4",
    "history": "4.7.2",
    "html-webpack-plugin": "2.30.1",
    "jest": "21.2.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "4.1.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "15.6.0",
    "raf": "3.4.0",
    "react": "16.1.1",
    "react-dev-utils": "4.2.1",
    "react-dom": "16.1.1",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.8",
    "redux": "3.7.2",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start-js": "node scripts/start.js",
    "start-dev": "npm-run-all -p watch-css start-js",
    "start": "node server.js",
    "heroku-postbuild": "npm run build",
    "build": "npm run build-css && node scripts/build.js",
    "build-css": "node-sass-chokidar src/scss/ -o src/css/",
    "watch-css": "npm run build-css && node-sass-chokidar src/scss/ -o src/css/ --watch --recursive",
    "lint": "eslint ./src/**/*.js",
    "test": "cross-env NODE_ENV=test CI=true node scripts/test.js --env=jsdom --color",
    "cover": "npm test -- --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "4.11.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-react": "7.5.1",
    "pre-commit": "1.2.2",
    "react-test-renderer": "16.1.1",
    "redux-devtools": "3.4.1",
    "sinon": "4.1.2"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/components/Router/Router.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/testSetup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "keywords": [
    "react",
    "redux",
    "redux-thunk",
    "react-redux-boilerplate",
    "react-docker",
    "react-travis",
    "react-heroku"
  ],
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
